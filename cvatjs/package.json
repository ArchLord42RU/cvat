{
  "name": "cvat.js",
  "version": "1.0.2",
  "description": "Part of Computer Vision Tool which presents an interface for client-side integration",
  "main": "babel.config.js",
  "scripts": {
    "babel": "mkdir babel.build -p && babel src -d babel.build",
    "webpack": "webpack",
    "build": "npm version patch && npm run babel && npm run webpack && rm babel.build -r",
    "test": "npm run build && node --nolazy --inspect-brk=9229 tests.js",
    "debug": "node --nolazy --inspect-brk=9230 src/api.js",
    "docs": "jsdoc --readme README.md src/*.js -p -c jsdoc.config.js -d docs"
  },
  "author": "Intel",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "jsdoc": "^3.5.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "js-cookie": "^2.2.0"
  }
}
